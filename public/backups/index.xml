      }&#xA;        &lt;/script&gt;&#xA;&lt;/ul&gt;&#xA;&lt;/blockquote&gt;&#xA;&lt;h3 id=&#34;2023&#34;&gt;&lt;code&gt;2023&lt;/code&gt;&lt;/h3&gt;&#xA;&lt;blockquote&gt;&#xA;&lt;ul class=circle&gt;&#xA;       &lt;script&gt;&#xA;            var i;&#xA;            for (i = 0; i &lt; papers_full.length; i++) {&#xA;                if (papers_full[i].year == &#34;2023&#34;) {&#xA;                    document.write(&#34;&lt;li class=paper&gt;&#34;);&#xA;                    printPaper(papers_full[i], &#34;g&#34;);&#xA;                    document.write(&#34;&lt;/li&gt;&#34;);&#xA;                }&#xA;            }&#xA;        &lt;/script&gt;&#xA;&lt;/ul&gt;&#xA;&lt;/blockquote&gt;&#xA;&lt;h3 id=&#34;2022&#34;&gt;&lt;code&gt;2022&lt;/code&gt;&lt;/h3&gt;&#xA;&lt;blockquote&gt;&#xA;&lt;ul class=circle&gt;&#xA;        &lt;script&gt;&#xA;            var i;&#xA;            for (i = 0; i &lt; papers_full.length; i++) {&#xA;                if (papers_full[i].year == &#34;2022&#34;) {&#xA;                    document.write(&#34;&lt;li class=paper&gt;&#34;);&#xA;                    printPaper(papers_full[i], &#34;g&#34;);&#xA;                    document.write(&#34;&lt;/li&gt;&#34;);&#xA;                }&#xA;            }&#xA;        &lt;/script&gt;&#xA;&lt;/ul&gt;&#xA;&lt;/blockquote&gt;&#xA;&lt;h3 id=&#34;2021-&#34;&gt;&lt;code&gt;2021-&lt;/code&gt;&lt;/h3&gt;&#xA;&lt;blockquote&gt;&#xA;&lt;p&gt;&lt;span style=&#34;color:gray;font-size:1em;&#34;&gt; Selected publications before 2021 (Complete list: &lt;a href=&#34;https://scholar.google.com/citations?user=drR_WcAAAAAJ&amp;amp;hl=en&amp;amp;sortby=pubdate&#34; target=&#34;_blank&#34; rel=&#34;noopener&#34;&gt;Google Scholar&lt;/a&gt; | &lt;a href=&#34;https://dblp.org/pid/139/4363.html&#34; target=&#34;_blank&#34; rel=&#34;noopener&#34;&gt;DBLP&lt;/a&gt;) &lt;/span&gt;&lt;/p&gt;</description>
